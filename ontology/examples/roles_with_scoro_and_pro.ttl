@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix fabio: <http://purl.org/spar/fabio/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix frapo: <http://purl.org/cerif/frapo/> .
@prefix pro: <http://purl.org/spar/pro/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix scoro: <http://purl.org/spar/scoro/> .
@prefix ti: <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#> .
@prefix tvc: <http://www.essepuntato.it/2012/04/tvc/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<doi:dataset> a dcat:Dataset, prov:Entity;
   dcterms:contributor <orcid:you> .

<doi:article> a fabio:JournalArticle, dcat:Resource, prov:Entity ;
   dcterms:contributor <orcid:me> .

<uri:project> a frapo:Project ;
   dcterms:contributor <orcid:you>, <orcid:me> .

<orcid:me> a foaf:Person ;
   foaf:name "Me McMe" ;
   pro:holdsRoleInTime [ a pro:RoleInTime ;
      pro:withRole  scoro:corresponding-author ;
      pro:relatesToEntity <doi:article> ] ;  # HMR: using the subproperty pro:relatesToDocument implies that the range is a foaf:Document... overkill?
   pro:holdsRoleInTime [ a pro:RoleInTime ;
      pro:withRole  scoro:principal-investigator ;
      pro:relatesToEntity <uri:project> ;
      tvc:atTime [ a tvc:TimeInterval ;
        ti:hasIntervalStartDate "2017"^^xsd:dateTime ;
        ti:hasIntervalEndDate "2024"^^xsd:dateTime ] ] .

<orcid:you> a foaf:Person ;
   foaf:name "You McYou" ;
   pro:holdsRoleInTime [ a pro:RoleInTime ;
      pro:withRole  scoro:principal-investigator ;
      pro:relatesToEntity <uri:project> ;
      tvc:atTime [ a tvc:TimeInterval ;
        ti:hasIntervalStartDate "2024"^^xsd:dateTime ] ] .
